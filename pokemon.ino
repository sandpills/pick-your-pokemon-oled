const int buttonLPin = 2;
const int buttonUPin = 3;
const int buttonRPin = 4;
// debounce delay for pushbutton:
const int debounceDelay = 4;

int lastButtonLState = HIGH;
int lastButtonUState = HIGH;
int lastButtonRState = HIGH;

bool select1 = false;

void setup() {
  Serial.begin(115200);
  if(!display.begin(SSD1306_SWITCHCAPVCC, 0x3C)) {
    Serial.println(F("SSD1306 allocation failed"));
    for(;;);
  }
  delay(2000);
  Keyboard.begin();
  
  pinMode(buttonLPin, INPUT);
  pinMode(buttonUPin, INPUT);
  pinMode(buttonRPin, INPUT);
    
  display.setTextSize(1);
  display.setTextColor(WHITE);
  display.setCursor(13, 25);
  display.clearDisplay();
  display.drawRoundRect(10, 15, 108, 30, 2, WHITE);
  display.println("PICK YOUR POKEMON");
  display.display();
}

void loop() {

  int buttonLState = digitalRead(buttonLPin);
  if (buttonLState != lastButtonLState) {
    delay(debounceDelay);
    if (buttonLState == HIGH) {
      display.invertDisplay(false);
      display.clearDisplay();
      display.drawBitmap(0, 0, bitmap_pkchu, 128, 64, WHITE);
      display.display();
      delay(250);
      display.invertDisplay(true);
    }
    lastButtonLState = buttonLState;
   }

  int buttonRState = digitalRead(buttonRPin);
  if (buttonRState != lastButtonRState) {
    delay(debounceDelay);
    if (buttonRState == HIGH) {
      display.invertDisplay(false);
      display.clearDisplay();
      display.drawBitmap(0, 0, bitmap_sqtle, 128, 64, WHITE);
      display.display();
      delay(250);
      display.invertDisplay(true);
    }
      lastButtonRState = buttonRState;   
  }
    
  int buttonUState = digitalRead(buttonUPin);
  if (buttonUState != lastButtonUState) {
    delay(debounceDelay);
    if (buttonUState == HIGH) {
      display.invertDisplay(false);
      display.clearDisplay();
      display.drawBitmap(0, 0, bitmap_cmder, 128, 64, WHITE);
      display.display();
      delay(250);
      display.invertDisplay(true);
    }
      lastButtonUState = buttonUState;
  }
}
